{
  "name": "patternlab-react-demo",
  "version": "1.0.2",
  "description": "Demo site using patternlab-react",
  "main": "build/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf build; babel src --presets=babel-preset-es2015,react --plugins=transform-object-rest-spread --out-dir=build",
    "sm": "node -e 'require(\"@peteyg/patternlab-react\").generateSourcemap()'",
    "bundle": "rm public/app.js; browserify build/app.js -o public/app.js",
    "create": "npm run build; npm run sm; npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peteyg99/patternlab-react-demo.git"
  },
  "keywords": [
    "react",
    "patternlab"
  ],
  "author": "Pete Gaulton",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@peteyg/patternlab-react": "^1.0.16",
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "browserify": "^14.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  }
}
